{
  "name": "food-truck-ingestor",
  "version": "0.1.0",
  "private": true,
  "description": "CLI to ingest Pittsburgh food truck events via Firecrawl/Apify into Supabase",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
