  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "@types/yargs": "^17.0.33",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  }
}
